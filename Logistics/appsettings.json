{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "Authentication": {
        "AccessTokenExpirationSeconds": "600",
        "RefreshTokenExpirationHours": "24",
        "ApproveEmailTokenExpirationMinutes": "15",
        "ResetPasswordTokenExpirationMinutes": "15",
        "Issuer": "Logistics",
        "Audience": "Logistics/api"
    }
}